<p id="notice"><%= notice %></p>

<h1>Sheet Bs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sheet_bs.each do |sheet_b| %>
      <tr>
        <td><%= link_to 'Show', sheet_b %></td>
        <td><%= link_to 'Edit', edit_sheet_b_path(sheet_b) %></td>
        <td><%= link_to 'Destroy', sheet_b, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sheet B', new_sheet_b_path %>
